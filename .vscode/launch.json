{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "调试当前文件",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "D:/Environment/mingw64/bin/gdb.exe",
      "preLaunchTask": "build active file",
      "setupCommands": [
        {
          "description": "启用 gdb pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
